{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "ca.jaggernaut.midi-mixer.voicemeeter",
  "name": "Voicemeeter",
  "type": "node",
  "version": "2.1.0",
  "author": "Jaggernaut",
  "main": "lib/index.js",
  "icon": "voicemeeter.ico",
  "settings": {
    "notes": {
      "label": "Note",
      "type": "status",
      "fallback": "Reload plugin after changing any of these settings. Check the info tab for explanations of the customization options."
    },
    "maxdb": {
      "label": "Max db",
      "type": "slider",
      "fallback": 12,
      "min": -60,
      "max": 12
    },
    "mindb": {
      "label": "Min db",
      "type": "slider",
      "fallback": -60,
      "min": -60,
      "max": 12
    },
    "maxpan": {
      "label": "Max Pan",
      "type": "slider",
      "fallback": 12,
      "min": 1,
      "max": 12
    },
    "minpan": {
      "label": "Min Pan",
      "type": "slider",
      "fallback": -12,
      "min": -12,
      "max": -1
    },
    "reverbLevel": {
      "label": "Reverb Level",
      "type": "slider",
      "fallback": 10,
      "min": 0,
      "max": 10
    },
    "delayLevel": {
      "label": "Delay Level",
      "type": "slider",
      "fallback": 10,
      "min": 0,
      "max": 10
    },
    "customStripAssign": {
      "label": "Custom Strip Assign",
      "type": "text"
    },
    "customStripMute": {
      "label": "Custom Strip Mute",
      "type": "text"
    },
    "customStripRun": {
      "label": "Custom Strip Run",
      "type": "text"
    },
    "BusToggles": {
      "label": "Custom Bus Toggle Buttons",
      "type": "text",
      "fallback": "strip0:A1,!B2;strip1:A1"
    },
    "vmstatus": {
      "label": "Voicemeeter Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "runbutton": {
      "label": "run",
      "type": "button"
    }
  }
}